<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.czcps.oa.dao.DepartmentDao">
    <select id="selectById" parameterType="Long" resultType="cn.czcps.oa.entity.Department">
        select * from adm_department
        where departement_id=#{value}
    </select>
</mapper>